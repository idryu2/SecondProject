
/*
 * 		&(*)     |     ^
 * ======================
 * 00	0		0		0
 * ======================
 * 01	0		1		1
 * ======================
 * 10	0		1		1
 * ======================
 * 11	1		1		0
 * ======================
 * 
 * 10&5			 10|5			 10^5
 * 1010			1010			1010
 * 0101			0101			0101
 * ------		------			-----
 * 0000 = 0		1111  = 15		1111  15
 * 
 */




public class 비트연산자 {
	public static void main(String[] args){
		
		System.out.println(10^5);
	}

}
