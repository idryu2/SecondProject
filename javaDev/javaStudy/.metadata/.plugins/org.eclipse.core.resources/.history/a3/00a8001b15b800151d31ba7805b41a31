/**
 *
 * @author dmitryigoshin
 */
public class Shot {
    int X;
    int Y;

    Shot(int x, int y)
    {
        X = x;
        Y = y;
    }

    int getX ()
    {
	    return X;
    }

    int getY ()
    {
	    return Y;
    }

    void setX (x)
    {
	    X = x;
    }

void setY (y)
{
	Y = y;
}



}
